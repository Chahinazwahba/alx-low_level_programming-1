CC = gcc
CFLAGS = -Wall -Werror -pedantic -std=gnu90
OBJ = main.o 101-strtow.o string_functions.o

all: 101-strtow

101-strtow: $(OBJ)
        $(CC) $(CFLAGS) $(OBJ) -o 101-strtow

main.o: main.c main.h
        $(CC) $(CFLAGS) -c main.c

101-strtow.o: 101-strtow.c main.h
        $(CC) $(CFLAGS) -c 101-strtow.c

string_functions.o: string_functions.c main.h
        $(CC) $(CFLAGS) -c string_functions.c

clean:
        rm -f $(OBJ) 101-strtow

